{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:27057",
      "sslPort": 44339
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "BROWSER_ROOT_DIR": "./SampleData"
      }
    },
    "http": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "BROWSER_ROOT_DIR": "./SampleData"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5255"
    },
    "https": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "BROWSER_ROOT_DIR": "./SampleData"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7071;http://localhost:5255"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true,

      "containerRunArguments": "-v .\\src\\FileServer\\SampleData:/app/data"
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}